    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "name": "Python Main",
            "type": "python",
            "request": "launch",
            "program": "${fileDirname}/main.py",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "name": "Python Main Convert DS3",
            "type": "python",
            "request": "launch",
            "program": "${fileDirname}/main.py",
            "args": [
                "--name", "Bullet", "--copy" , "DS3"
            ],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        }
    ]
}